{
  "rewrites": [
    {
      "source": "/((?!.*\\.(?:js|css|json|png|jpg|jpeg|gif|svg|webp|woff|woff2|ttf|map)$).*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(js|css|woff2|woff|ttf|png|jpg|jpeg|gif|svg|webp|map)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
